{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "jsx": "react",
    "lib": ["es6", "es2019"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "target": "esnext",
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"],
      "@globalComponents/*": ["src/globalComponents/*"],
      "@resources/*": ["src/resources/*"],
      "@services/*": ["src/services/*"],

    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
